{
  "name": "@jy95/i18n-tools",
  "version": "1.0.0",
  "description": "CLI to make conversions from / to i18n files simpler",
  "bin": {
    "i18n-tools": "dist/cli.ts"
  },
  "scripts": {
    "cli": "npx ts-node src/cli.ts",
    "test": "npx jest",
    "lint": "npx eslint src/ --ext .js,.jsx,.ts,.tsx",
    "build": "npx tsc -p tsconfig.json",
    "clean": "rm -rf dist build",
    "ts-node": "npx ts-node"
  },
  "keywords": [
    "i18n",
    "xlsx",
    "conversion",
    "internalization",
    "export",
    "import"
  ],
