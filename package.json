    "@types/lodash": "^4.14.172",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^16.4.10",
    "@types/yargs": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "eslint": "^7.32.0",
    "fsify": "^4.0.2",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "exceljs": "^4.2.1",
